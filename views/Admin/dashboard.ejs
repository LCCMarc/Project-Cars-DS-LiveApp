<h1><%= __('dashboard') %></h1>
<div class="row">
  <div class="col-md-4 col-sm-6">
    <div class="panel panel-danger">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-users fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= drivers.length %></div>
            <div><%= __('drivers_registred') %></div>
          </div>
        </div>
      </div>
      <a href="#">
        <div class="panel-footer">
          <span class="pull-left"><%= __('view_details') %></span>
          <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
          <div class="clearfix"></div>
        </div>
      </a>
    </div>
  </div>
  <div class="col-md-4 col-sm-6">
    <div class="panel panel-danger">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-history fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= laps.length %></div>
            <div><%= __('laps_done') %></div>
          </div>
        </div>
      </div>
      <a href="#">
        <div class="panel-footer">
          <span class="pull-left"><%= __('view_details') %></span>
          <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
          <div class="clearfix"></div>
        </div>
      </a>
    </div>
  </div>
  <div class="col-md-4 col-sm-6">
    <div class="panel panel-danger">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-flag-checkered fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= sessions.length %></div>
            <div><%= __('sessions_saved') %></div>
          </div>
        </div>
      </div>
      <a href="#">
        <div class="panel-footer">
          <span class="pull-left"><%= __('view_details') %></span>
          <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
          <div class="clearfix"></div>
        </div>
      </a>
    </div>
  </div>
</div>
<!-- /.row -->
<div class="row">
  <div class="col-md-6">
    <div class="panel panel-danger">
      <div class="panel-heading"><%= __('last_sessions') %></div>
      <div class="panel-body">
        <%= __('not_implemented_yet') %>
      </div>
    </div>
  </div>
  <div class="col-md-6">
    <div class="panel panel-danger">
      <div class="panel-heading"><%= __('last_users') %></div>
      <div class="panel-body">
        <%= __('not_implemented_yet') %>
      </div>
    </div>
  </div>
</div>
