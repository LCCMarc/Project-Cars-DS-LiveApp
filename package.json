    "render-mustache": "sailjs/render-mustache",
    "request": "^2.58.0",
    "request-json": "^0.5.3",
    "sails": "~0.11.0",
    "sails-disk": "~0.10.8",
    "sails-memory": "^0.10.5",
    "sails-mongo": "^0.11.2",
    "sails-mysql": "~0.10.12",
    "sails-postgresql": "^0.10.15",
    "skipper-disk": "^0.5.4",
    "validator": "^3.41.2"
